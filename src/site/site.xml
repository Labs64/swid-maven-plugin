<project xmlns="http://maven.apache.org/DECORATION/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.1.0 http://maven.apache.org/xsd/decoration-1.1.0.xsd"
         name="swid-maven-plugin">
    <publishDate format="dd MMM yyyy" position="right" />
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.1</version>
    </skin>
    <custom>
        <fluidoSkin>
            <profile>pre-release</profile>

            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <breadcrumbDivider>Â»</breadcrumbDivider>
            <gitHub>
                <projectId>Labs64/swid-maven-plugin</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>#973c05</ribbonColor>
            </gitHub>
            <googleSearch>
                <sitesearch />
            </googleSearch>
            <ohloh>
                <projectId>swid-maven-plugin</projectId>
                <widget>thin-badge</widget>
            </ohloh>
        </fluidoSkin>
    </custom>

    <bannerLeft>
        <name>${project.name}</name>
        <src>/images/Labs64_LogoS.png</src>
        <href>http://www.labs64.com</href>
    </bannerLeft>
    <bannerRight>
        <src></src>
    </bannerRight>

    <body>
        <breadcrumbs>
            <item name="SWID Tags Generator for Maven" href="https://github.com/Labs64/swid-maven-plugin" />
        </breadcrumbs>
        <links>
            <item name="Labs64" href="http://www.labs64.com" />
        </links>
        <menu name="Overview">
            <item name="Introduction" href="index.html" />
            <item name="Goals" href="plugin-info.html" />
