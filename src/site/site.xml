            <item name="FAQ" href="faq.html" />
        </menu>
        <menu name="Examples">
            <item name="Generate SWID Tag" href="examples/generate.html" />
        </menu>
        <menu name="Developer Info">
            <item name="Releases" href="https://github.com/Labs64/swid-maven-plugin/releases" />
            <item name="Issues" href="https://github.com/Labs64/swid-maven-plugin/issues" />
            <item name="Forum" href="https://groups.google.com/group/swid-maven-plugin" />
        </menu>
        <menu ref="reports" />
        <menu name="External Links">
            <item name="SWID Tags Generator (Java Library)" href="https://github.com/Labs64/swid-generator/" />
